{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT"
  },
  "mapproxy": {
    "configProvider": "MAPPROXY_CONFIG_PROVIDER",
    "cache": {
      "grids": "MAPPROXY_CACHE_GRIDS",
      "requestFormat": "MAPPROXY_CACHE_REQUEST_FORMAT",
      "upscaleTiles": {
        "__name": "MAPPROXY_CACHE_UPSCALE_TILES",
        "__format": "number"
      },
      "directoryLayout": "MAPPROXY_CACHE_DIRECTORY_LAYOUT"
    }
  },
  "FS": {
    "yamlFilePath": "MAPPROXY_YAML_FILE_PATH",
    "internalMountDir":  "INTERNAL_MOUNT_DIR",
    "rootTilesPath": "ROOT_TILES_PATH"
  },
  "S3": {
    "accessKeyId": "S3_ACCESS_KEY_ID",
    "secretAccessKey": "S3_SECRET_ACCESS_KEY",
    "endpointUrl": "S3_ENDPOINT_URL",
    "bucket": "S3_BUCKET",
    "sslEnabled": "S3_SSL_ENABLED"
  },
  "DB": {
    "host": "DB_HOST",
    "port": "DB_PORT",
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "database": "DB_NAME",
    "enableSslAuth": {
      "__name": "DB_SSL_ENABLE",
      "__format": "boolean"
    },
    "rejectUnauthorized": "DB_SSL_REJECT_UNAUTHORIZED",
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    }
  }
}
