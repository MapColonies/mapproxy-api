{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT"
  },
  "mapproxy": {
    "fileProvider": "MAPPROXY_FILE_PROVIDER",
    "cache": {
      "grids": "MAPPROXY_CACHE_GRIDS",
      "requestFormat": "MAPPROXY_CACHE_REQUEST_FORMAT",
      "upscaleTiles": "MAPPROXY_CACHE_UPSCALE_TILES",
      "directoryLayout": "MAPPROXY_CACHE_DIRECTORY_LAYOUT"
    }
  },
  "FS": {
    "yamlFilePath": "MAPPROXY_YAML_FILE_PATH"
  },
  "S3": {
    "awsAccessKeyId": "AWS_ACCESS_KEY_ID",
    "awsSecretAccessKey": "AWS_SECRET_ACCESS_KEY",
    "endpointUrl": "AWS_ENDPOINT_URL",
    "bucket": "AWS_BUCKET",
    "sslEnabled": "AWS_SSL_ENABLED"
  },
  "DB": {
    "host": "DB_HOST",
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "database": "DB_DATABASE",
    "enableSslAuth": "DB_SSL_ENABLE",
    "rejectUnauthorized": "DB_SSL_REJECT_UNAUTHORIZED",
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    }
  }
}
