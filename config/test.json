{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "configProvider": "fs",
    "cache": {
      "grids": "epsg4326dir",
      "upscale_tiles": 18,
      "type": "s3",
      "directory_layout": "tms"
    }
  },
  "redis": {
    "enabled": true,
    "host": "raster-mapproxy-redis-master",
    "port": 6379,
    "enableRedisUser": true,
    "username": "mapcolonies",
    "password": "mapcolonies",
    "enablePrefix": true,
    "prefix": "mcrl:",
    "type": "redis",
    "default_ttl": 86400
  }
}
