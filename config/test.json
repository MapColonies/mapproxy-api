{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "configProvider": "fs",
    "cache": {
      "grids": "epsg4326dir",
      "format": "image/png",
      "upscale_tiles": 18,
      "type": "s3",
      "directory_layout": "tms"
    }
  }
}
