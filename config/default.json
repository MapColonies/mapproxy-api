{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "debug"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "yamlFilePath": "",
    "fileProvider": "db",
    "cache": {
      "grids": "epsg4326ul",
      "requestFormat": "image/png",
      "upscaleTiles": 18,
      "directoryLayout": "tms",
      "gpkgExt": ".gpkg"
    }
  },
  "S3": {
    "awsAccessKeyId": "minioadmin",
    "awsSecretAccessKey": "minioadmin",
    "endpointUrl": "http://localhost:9000",
    "bucket": "dtm",
    "objectKey": "mapproxy.yaml",
    "sslEnabled": false
  },
  "DB": {
    "host": "localhost",
    "user": "postgres",
    "password": "postgres",
    "database": "",
    "port": 5432,
    "table": "configurations",
    "columns": {
      "data": "data"
    }
  }
}
