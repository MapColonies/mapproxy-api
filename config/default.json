{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "fileProvider": "",
    "cache": {
      "grids": "epsg4326ul",
      "requestFormat": "image/png",
      "upscaleTiles": 18,
      "directoryLayout": "tms",
      "gpkgExt": ".gpkg"
    }
  },
  "FS": {
    "yamlFilePath": ""
  },
  "S3": {
    "awsAccessKeyId": "minioadmin",
    "awsSecretAccessKey": "minioadmin",
    "endpointUrl": "http://localhost:9000",
    "bucket": "",
    "objectKey": "mapproxy.yaml",
    "sslEnabled": false
  },
  "DB": {
    "host": "localhost",
    "user": "postgres",
    "password": "postgres",
    "database": "",
    "port": 5432,
    "table": "configurations",
    "columns": {
      "data": "data"
    },
    "enableSslAuth": false,
    "rejectUnauthorized": true,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
