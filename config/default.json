{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "fileExt": ".yaml",
    "defaultFilePath": "config/default.yaml",
    "yamlFilePath": "/path/to/config/file.yaml",
    "cache": {
      "grids": ["epsg4326dir"],
      "requestFormat": "image/png",
      "upscaleTiles": 18,
      "type": "s3",
      "directoryLayout": "tms"
    },
    "s3": {
      "endpointUrl": "http://localhost:9000",
      "bucket": "mybucket"
    }
  }
}
