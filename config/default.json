{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "jsonPath": "/api.json",
    "uiPath": "/api"
  },
  "logger": {
    "level": "info"
  },
  "server": {
    "port": "8080"
  },
  "mapproxy": {
    "fileProvider": "fs",
    "fileExt": ".yaml",
    "defaultFilePath": "config/default.yaml",
    "yamlFilePath": "/default.yaml",
    "cache": {
      "grids": "epsg4326ul",
      "requestFormat": "image/png",
      "upscaleTiles": 18,
      "directoryLayout": "tms",
      "gpkgExt": ".gpkg"
    },
    "s3": {
      "awsAccessKeyId": "minioadmin",
      "awsSecretAccessKey": "minioadmin",
      "endpointUrl": "http://localhost:9000",
      "storageBucket": "storage_bucket",
      "configFileBucket": "config_file_bucket",
      "objectKey": "mapproxy.yaml",
      "sslEnabled": false
    }
  }
}
